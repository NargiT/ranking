{
  "name": "ranking-webapp",
  "version": "1.0.0",
  "description": "Ranking system web application",
  "main": "index.js",
  "dependencies": {
    "jquery": "^2.2.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "material-ui":"^0.14.4"
  },
  "devDependencies": {
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "http-server": "^0.9.0",
    "browserify":"^13.0.0"
  },
  "scripts": {
    "test": "nothing to run",
    "build:js": "browserify -t [ babelify --presets [ react ] ] assets/scripts/*.js -o dist/app.js",
    "build:html": "cp assets/html/index.html dist/index.html",
    "build:css": "cp assets/css/*.css dist/style.css",
    "build": "npm run build:js & npm run build:html & npm run build:css",
    "start-http": "node node_modules/http-server/bin/http-server dist",
    "build:run": "npm run build & npm run start-http"
  },
  "author": "Tigran",
  "license": "APACHE 2.0"
}
